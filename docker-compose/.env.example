##### Docker Images #####
ACAPY_IMAGE=bcgovimages/aries-cloudagent:py36-1.16-1_0.7.3
POSTGRES_IMAGE=postgres:13.1-alpine
KEYCLOAK_IMAGE=quay.io/keycloak/keycloak:19.0.1

##### Controller #####
CONTROLLER_WEBHOOK_API_KEY=secret-key

CONTROLLER_DB_DATABASE_NAME=controller
CONTROLLER_DB_USER=controller
CONTROLLER_DB_PASSWORD=controller

##### Aca-Py #####
ACAPY_DB_DATABASE_NAME=acapy
ACAPY_DB_USER=acapy
